

protocol click{
import idl "common.inc";
record ClickRequest {
    // 标记本次请求用户的cookiesId
     string cookiesId;
    // 用户基本信息
    // 年龄
     int age;

    // 性别 0:未知 1:男 2:女
     int sex;

    // 地理信息
     GeoInfo geoInfo;

    // 广告信息
     AdInfo adInfo;
}

record ClickResponse {
    // 返回本次用户请求的cookiesId
     string cookiesId;

    // 用户基本信息
    // 年龄
     int age;

    // 性别 0:未知 1:男 2:女
     int sex;

    // 地理信息
     GeoInfo geoInfo;

    // 广告信息
     AdInfo adInfo;
}
}
